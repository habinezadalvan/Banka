{
  "name": "banka",
  "version": "1.0.0",
  "description": "Banka is a light-weight core banking application that powers banking operations like account",
  "main": "app.js",
  "scripts": {
    "test": "nyc --reporter=html --reporter=text mocha --require babel-polyfill --require @babel/register --timeout 5000 server/tests/*.js --exit",
    "start": "nodemon --exec babel-node server/app.js",
    "startdb": "nodemon --exec babel-node database/app.database.js",
    "createadmin": "node --require @babel/register database/controllers/admin.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "createTables": "node --require @babel/register database/models/tables.js",
    "droptables": "node --require @babel/register database/models/droptables.js",
    "database": "concurrently \"npm run droptables\" \"npm run createTables\" \"npm run createadmin\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/habinezadalvan/Banka.git"
  },
  "author": "christian habineza",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/habinezadalvan/Banka/issues"
  },
  "homepage": "https://github.com/habinezadalvan/Banka#readme",
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.2",
    "@babel/register": "^7.4.0",
    "babel-polyfill": "^6.26.0",
    "bcrypt": "^3.0.5",
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "nodemon": "^1.18.10",
    "pg": "^7.10.0",
    "swagger-ui-express": "^4.0.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "make-runnable": "^1.3.6",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^14.0.0"
  }
}
